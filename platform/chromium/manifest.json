{
	"name": "Blokker",
	"version": "BLOKKER_VERSION",
	"description": "Blocks YouTube, Twitter and Facebook feed.",
	"content_scripts": [
		{
      "run_at": "document_end",
			"matches": [
				"*://*.youtube.com/*",
				"*://*.twitter.com/*",
				"*://*.facebook.com/*"
			],
			"js": ["blokker.js"],
			"css": ["blokker.css"]
		}
	],
	"manifest_version" : 2,
	"icons": {
		"48": "img/icon48.png",
		"128": "img/icon128.png",
		"1024": "img/icon1024.png"
	}
}
